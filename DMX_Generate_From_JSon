		<script>
        
			function myOnLoad()
			{
				// console.log("Page chargée");
				//console.log("Lancement de DMX_HTML_Render");
				DMX_HTML_Render();
                
				// console.log("Lancement de DMX_JSon_Render");
				DMX_JSon_Render();
                
			}
			
		</script>
        
		<script>
			function DMX_HTML_Render()
			{
				console.log("Execution de DMX_HTML_Render");
                let HTML_Render = HTML_Convert(UniDMX); 
				document.getElementById("DMX_HTML_Render").innerHTML = HTML_Render;
			}
			
			function DMX_JSon_Render()
			{
				// console.log("Execution de DMX_JSon_Render");
				let JSon_Render = JSON.stringify(UniDMX);                
				document.getElementById("DMX_JSon_Render").innerHTML = JSon_Render;
			}

            function HTML_Convert(UniDMX)
            {
            	let HTML_Dump = "";
                
                for (let i=0; i < UniDMX.Uni.length ; i++) {
                HTML_Dump += "<h2>Univers : " + UniDMX.Uni[i].ID + "</h2>";  
                HTML_Dump += "<p>Nombre appareils " + UniDMX.Uni[i].NbrApp + "</p>";
                HTML_Dump += "<h3>Appareil " + UniDMX.Uni[i].App.AppID + " - " + UniDMX.Uni[i].App.AppName + "</h3>";
                HTML_Dump += "<ul>";
				HTML_Dump += "<li>Canal DMX : " + UniDMX.Uni[i].App.PosDMX + "</li>";
                HTML_Dump += "<li>Canaux DMX : " + UniDMX.Uni[i].App.CnxDMX + "</li>";
                HTML_Dump += "</ul>";
                HTML_Dump += "<ol>";
                for (let j=0; j < UniDMX.Uni[i].App.Can.length ; j++) {
               	HTML_Dump += "<li>" + UniDMX.Uni[i].App.CanName[j] + " : " + UniDMX.Uni[i].App.Can[j] + "</li>";
                HTML_Dump += "<div class=\"slidecontainer\"><input type=\"range\" min=\"0\" max=\"127\" value=\"" + UniDMX.Uni[i].App.Can[j] + "\" class=\"slider\" id=\"U" + i + 1 + "_App" + UniDMX.Uni[i].App.AppID + "_DMX" + j + 1 + "\"></div>";
                }
             	HTML_Dump += "</ol><br>";
                HTML_Dump += "<hr>";
                }
                return HTML_Dump;
            }
            
		</script>
    </head>
	
